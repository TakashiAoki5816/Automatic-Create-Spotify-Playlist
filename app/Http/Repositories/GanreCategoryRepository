<?php

namespace App\Http\Repositories;

use App\Models\GenreCategory;

class GenreCategoryRepository
{
    protected $genreCategoryRepository;

    /**
     * GenreCategoryRepository Constructor
     *
     * @param GenreCategoryRepository $genreCategoryRepository
     */
    public function __construct(GenreCategoryRepository $genreCategoryRepository)
    {
        $this->genreCategoryRepository = $genreCategoryRepository;
    }

    public function genreCategories()
    {
        $aa = GenreCategory::all()->pluck(['name', 'view_order']);
    }
}
